{
  "name": "tez-ui",
  "version": "0.0.1",
  "description": "Apache Tez UI",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "TMPDIR=tmp node ./node_modules/ember-cli/bin/ember build",
    "start": "TMPDIR=tmp node ./node_modules/ember-cli/bin/ember server",
    "test": "TMPDIR=tmp node ./node_modules/ember-cli/bin/ember test",

    "build:mvn": "TMPDIR=tmp node/node --harmony ./node_modules/ember-cli/bin/ember build -prod",
    "test:mvn": "TMPDIR=tmp node/node ./node_modules/ember-cli/bin/ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://git-wip-us.apache.org/repos/asf/tez.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {},
  "devDependencies": {
    "bower": "1.8.0",
    "grunt": "~0.4.1",
    "grunt-cli": "~0.1.13",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-uglify": "~0.2.0",
    "grunt-contrib-jshint": "~0.6.3",
    "grunt-contrib-cssmin": "~0.6.0",
    "grunt-contrib-connect": "~0.3.0",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-htmlmin": "~0.1.3",
    "grunt-contrib-watch": "~0.5.2",
    "grunt-rev": "~0.1.0",
    "grunt-usemin": "~0.1.12",
    "grunt-mocha": "~0.4.1",
    "grunt-open": "~0.2.0",
    "grunt-svgmin": "~0.2.0",
    "grunt-concurrent": "~0.3.0",
    "load-grunt-tasks": "~0.1.0",
    "connect-livereload": "~0.2.0",
    "grunt-ember-templates": "0.4.14",
    "time-grunt": "~0.1.1",
    "grunt-replace": "~0.4.4",
    "jshint-stylish": "~0.1.3",
    "grunt-neuter": "~0.6.0",
    "grunt-contrib-less": "~0.11"
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
